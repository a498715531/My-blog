<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace= "com.hjj.dao.mapper.SysUserMapper" >


    <select id="FindUserByAccount" resultType="com.hjj.dao.pojo.SysUser">
        select * from ms_sys_user where account = #{account}
    </select>
    <select id="FindUserBynickname" resultType="com.hjj.dao.pojo.SysUser" parameterType="java.lang.String">
        select * from ms_sys_user where nickname = #{nickname}
    </select>
</mapper>